#include <stdio.h>
#include <stdlib.h>
#include<string.h>

int main()
{
   char n[20];
   int c,a[20],i,j;
   scanf("%s",&n);
   c=strlen(n);
   for(i=0;i<c;i++){
    scanf("%d",&a[i]);
   }
   for(i=0;i<c;i++){
        for(j=0;j<=i;j++){
            n[j]=n[j]+a[i];
        }
        printf("%s\n",n);
   }

}
